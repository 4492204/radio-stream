{
  "name": "RadioStream",
  "version": "0.0.1",
  "private": true,
  "main": "main.electron.js",
  "###": "per https://medium.freecodecamp.com/building-an-electron-application-with-create-react-app-97945861647c",
  "homepage": "./",
  "scripts": {
    "web": "node webpack/scripts/start.js",
    "web-build": "node webpack/scripts/build.js",
    "android": "adb logcat -c && react-native run-android && ./logcat.sh",
    "android-reload": "adb shell am force-stop com.radiostream && adb shell monkey -p com.radiostream -c android.intent.category.LAUNCHER 1 && sleep 2 && adb shell input text \"RR\" && ./logcat.sh",
    "desktop": "concurrently 'while true; do node webpack/scripts/start.js --electron; done;' 'wait-on tcp:localhost:3000 && while true; do ELECTRON_START_URL=http://localhost:3000 node_modules/.bin/electron .; done;'",
    "desktop-build": "node webpack/scripts/build.js --electron && electron-packager --overwrite --out=build/electron --icon=js/app/images/app.icns .",
    "precommit": "lint-staged",
    "web-test": "NODE_PATH=./js ./node_modules/.bin/mocha --require babel-register --require babel-polyfill --reporter=min --recursive js/test"
  },
  "lint-staged": {
    "gitDir": "../",
    "*.js": [
      "prettier --write --trailing-comma es5 --jsx-bracket-same-line true --print-width 120",
      "git add"
    ]
  },
  "dependencies": {
    "autoprefixer": "6.7.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
